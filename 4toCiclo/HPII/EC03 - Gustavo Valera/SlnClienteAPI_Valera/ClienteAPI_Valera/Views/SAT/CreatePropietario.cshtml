@model ClienteAPI_Valera.Models.Propietario

@{
    ViewData["Title"] = "CreatePropietario";
}

<h1>CreatePropietario</h1>

<h4>Propietario</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreatePropietario">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Dnipro" class="control-label"></label>
                <input asp-for="Dnipro" class="form-control" />
                <span asp-validation-for="Dnipro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nompro" class="control-label"></label>
                <input asp-for="Nompro" class="form-control" />
                <span asp-validation-for="Nompro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Distrito" class="control-label"></label>
                <input asp-for="Distrito" class="form-control" />
                <span asp-validation-for="Distrito" class="text-danger"></span>
            </div>

            <br/>
           
            <div class="form-group">
                <input type="submit" value="Grabar" class="btn btn-primary" /> |
                <a asp-action="IndexPropietario" class="btn btn-warning">Regresar</a>
            </div>
        </form>
    </div>
</div>
 
<div>
    <b>@ViewBag.mensaje</b>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
