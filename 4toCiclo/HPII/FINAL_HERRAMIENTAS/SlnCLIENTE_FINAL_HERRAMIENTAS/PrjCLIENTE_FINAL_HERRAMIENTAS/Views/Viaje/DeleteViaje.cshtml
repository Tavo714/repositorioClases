@model PrjCLIENTE_FINAL_HERRAMIENTAS.Models.Viaje

@{
    ViewData["Title"] = "DeleteViaje";
}

<h1>DeleteViaje</h1>

<h3>Estas seguro de Eliminar este Viaje?</h3>
<div>
    <h4>Viaje</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NroVia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NroVia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NroBus)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NroBus)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodRut)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodRut)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodChof)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodChof)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FecVia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FecVia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CostoVia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CostoVia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Eliminado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Eliminado)
        </dd>
    </dl>
    
    <form asp-action="DeleteViaje">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="IndexViaje">Regresar</a>
    </form>
</div>
<br/>
<div>
    <b>@ViewBag.mensaje</b>
</div>
