@model PrjClienteFinalHerramientas.Models.Prestamo

@{
    ViewData["Title"] = "DeletePrestamo";
}

<h1>DeletePrestamo</h1>

<h3>Estas seguro de Eliminar este Prestamo?</h3>
<div>
    <h4>Prestamo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PrestamoId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PrestamoId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UsuarioId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LibroId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LibroId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TiempoPrestamo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TiempoPrestamo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
    </dl>
    
    <form asp-action="DeletePrestamo">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="IndexPrestamo">Regresar</a>
    </form>
</div>
<br/>
<div>
    <b>@ViewBag.mensaje</b>
</div>
